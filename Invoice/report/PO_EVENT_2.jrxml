	<queryString>
		<![CDATA[SELECT
    pe.PO_EVENT_ID,
    pe.PO_EVENT_NO,
    k.NO_KONTRAK,
    pe.KEGIATAN,
    pe.TANGGAL,
    pe.JUMLAH,
    pe.KETERANGAN,
    pe.BUKTI,
    de.*,
    dr.URAIAN AS URAIA,
    dr.DETAIL AS DETAI,
    dr.HARGA AS HARG,
    tr.*
FROM
    PO_EVENT pe
        INNER JOIN
    KONTRAK k ON pe.KONTRAK_ID = k.NO_KONTRAK
        INNER JOIN
    DETAIL_EVENT de ON pe.PO_EVENT_NO = de.PO_EVENT_NO
        INNER JOIN
    DETAIL_REIMBURSE dr ON pe.PO_EVENT_NO = dr.PO_EVENT_NO
        INNER JOIN
    TAGIHAN_REIMBURSE tr ON pe.PO_EVENT_NO = tr.PO_EVENT_NO
WHERE
    pe.TANGGAL BETWEEN $P{date1} AND $P{date2};]]>
	</queryString>
