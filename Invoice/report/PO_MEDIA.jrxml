	<queryString>
		<![CDATA[SELECT
    k.NO_KONTRAK,
    mm.MEDIA_NAME,
    pm.PO_MEDIA_ID,
    pm.PO_MEDIA_NO,
    pm.PEKERJAAN_KEMENTERIAN,
    pm.TANGGAL_TAYANG,
    pm.UKURAN,
    pm.HARGA,
    pm.PPN,
    pm.KETERANGAN,
    pm.PPN_IMAGE,
    tm.TAGIHAN_MEDIA_ID,
    tm.INVOICE_MEDIA,
    tm.TANGGAL,
    tm.NILAI_TAGIHAN,
    tm.PPN_IMAGE
FROM
    PO_MEDIA pm
        INNER JOIN
    KONTRAK k ON pm.KONTRAK_ID = k.KONTRAK_ID
        INNER JOIN
    MASTER_MEDIA mm ON pm.MASTER_MEDIA_ID = mm.MASTER_MEDIA_ID
        INNER JOIN
    TAGIHAN_MEDIA tm ON pm.PO_MEDIA_NO = tm.PO_MEDIA_NO
        INNER JOIN
    MASTER_PERUSAHAAN mp ON k.MASTER_PERUSAHAAN_ID = mp.MASTER_PERUSAHAAN_ID
WHERE
    mp.NAME = $P{perusahaan} OR k.PROJECT = $P{klien} AND
    pm.TANGGAL_TAYANG = $P{date1};]]>
	</queryString>
