	<queryString>
		<![CDATA[SELECT
    pp.PO_PRODUKSI_ID,
    pp.PO_PRODUKSI_NO,
    k.NO_KONTRAK,
    pp.PRODUKSI,
    pp.TANGGAL,
    pp.NILAI_PRODUKSI,
    pp.KETERANGAN,
    pp.PPN_IMAGE,
    dp.*
FROM
    PO_PRODUKSI pp
        INNER JOIN
    KONTRAK k ON pp.KONTRAK_ID = k.KONTRAK_ID
        INNER JOIN
    DETAIL_PRODUKSI dp ON pp.PO_PRODUKSI_NO = dp.PO_PRODUKSI_NO
        INNER JOIN
    MASTER_PERUSAHAAN mp ON k.MASTER_PERUSAHAAN_ID = mp.MASTER_PERUSAHAAN_ID
WHERE
    mp.NAME = $P{perusahaan} OR k.PROJECT = $P{klien} AND
    pp.TANGGAL BETWEEN $P{date1} AND $P{date2};]]>
	</queryString>
