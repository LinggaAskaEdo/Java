	<queryString>
		<![CDATA[SELECT
    poe.PO_EVENT_ID,
    poe.PO_EVENT_NO,
    poe.KONTRAK_ID,
    poe.KEGIATAN,
    poe.TANGGAL,
    poe.JUMLAH,
    poe.KETERANGAN,
    poe.BUKTI,
    de.DETAIL_EVENT_ID,
    de.PO_EVENT_NO,
    de.URAIAN,
    de.DETAIL,
    de.VOL_1,
    de.JENIS_1,
    de.VOL_2,
    de.JENIS_2,
    de.HARGA_SATUAN,
    de.TOTAL
FROM
    PO_EVENT AS poe
        JOIN
    DETAIL_EVENT AS de ON poe.PO_EVENT_NO = de.PO_EVENT_NO
WHERE
    poe.TANGGAL BETWEEN $P{date} AND $P{dete_2};]]>
	</queryString>
