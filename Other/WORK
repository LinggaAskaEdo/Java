<script type="text/javascript">showAlert2('Data Not Founds', this);</script>

function showAlert2(str,action)
{
	var _id = 'dialog-' + generateRandomString();
	var selector = '#'+_id;
	var _str= str || "";
	var anchor = anchor || "";
	var _tpl = '<div id="'+_id+'" style="text-align:right;"><table width="100%" border="0" cellpadding="2" cellspacing="2"><tr><td valign="middle" align="center"><img src="images/red-exclamation.gif" alt="Alert" /></td><td valign="middle" class="center-align" style="white-space:pre-line" ><span class="contentText">'+_str+'</span></td></tr></table></div>';

	$(selector).dialog('destroy');
    $('body').append(_tpl);
    $(selector).dialog
    ({    	
        autoOpen: true,
        resizable: false,
        modal: true,
        width: 200,
        title:alertMsg.common_msg_confirm_title
    });	
}

