/*@PostConstruct
	public void loadSingleConfig()
	{
		GetValueRes resSingle = new GetValueRes();
		
		GetValueReq reqSingle = new GetValueReq();
		reqSingle.setModule("ADMIN");
		reqSingle.setKey("user.and.target.msisdn.duplication.checking");
		
		resSingle = impl.getSingleValue(reqSingle, GetValueRes.class, urlSingleValue, timeoutMS);
		if (resSingle != null)
		{
			logger.info(resSingle.getData().getValue().toString());
			pfcApi.USER_AND_TARGET_MSISDN_DUPLICATION_CHECKING = resSingle.getData().getValue().toString();
		}
	}*/
	
	@PostConstruct
	public void loadMultiConfig() throws IllegalArgumentException, IllegalAccessException, NoSuchFieldException, SecurityException
	{
		int timeoutMS = pfc.CONFIG_API_TIME_OUT;
		String urlMultipleValue = pfc.CONFIG_API_URL_MULTIPLE_VALUE;
		
		MultiGetValueRes resMultiple = new MultiGetValueRes();
		
		String listKey[] = {"absolute.group.separation", "sso.superadmin.limited", "user.and.target.msisdn.duplication.checking"};
		
		MultiGetValueReq reqMultiple = new MultiGetValueReq();
		reqMultiple.setModule("ADMIN");
		reqMultiple.setKeys(listKey);
		
		resMultiple = impl.getMultiValue(reqMultiple, MultiGetValueRes.class, urlMultipleValue, timeoutMS);
		if (resMultiple != null)
		{
			String key, value, type;			
			Object parseVal;
			
			for (int i = 0; i < resMultiple.getData().getKeys().size(); i++)
			{
				key = resMultiple.getData().getKeys().get(i).getKey().toString();
				value = resMultiple.getData().getKeys().get(i).getValue().toString();
				type = resMultiple.getData().getKeys().get(i).getType().toString();
				
				logger.info("key: {}, value: {}, type: {}", key, value, type);
								
				if (type.equalsIgnoreCase("int"))
				{
					parseVal = Integer.parseInt(value);
				}
				else if (type.equalsIgnoreCase("boolean"))
				{
					parseVal = Boolean.parseBoolean(value);
				}
				else
				{
					parseVal = value;
				}
				
				PreferenceApi.class.getDeclaredField(PreferenceApi.listFieldKey.get(key).toString()).set(pfcApi, parseVal);
			}
		}

