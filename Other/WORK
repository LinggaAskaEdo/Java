SELECT 
    -- al.AUDIT_LOG_ID id,
    -- ul.USER_LOG_ID,
    -- al.TIMESTAMP,
    IFNULL(u.USER_NAME, ul.USER_NAME) AS USER_NAME,
    al.MODULE,
    al.TARGET_MSISDN AS MSISDN,
    -- av.LEVEL,
    al.AUDIT_MESSAGE,
    GROUP_CONCAT(r.ROLE_NAME),
    ul.TOKEN -- ,
    -- GROUP_CONCAT(an.AUDIT_NOTES_ID SEPARATOR ',') AS AUDIT_NOTES_IDS, COUNT(AUDIT_NOTES_ID) COUNT_NOTES
FROM
    LRTP_V2.AUDIT_LOG al INNER JOIN
    -- LRTP_V2.AUDIT_LEVEL av ON al.AUDIT_LEVEL_ID = av.AUDIT_LEVEL_ID INNER JOIN
    LRTP_V2.USER_LOG ul ON al.USER_LOG_ID = ul.USER_LOG_ID LEFT JOIN
    LRTP_V2.USER u ON ul.USER_ID = u.USER_ID INNER JOIN 
	LRTP_V2.USER_ROLE_MAPPING urm ON u.USER_ID = urm.USER_ID INNER JOIN 
	LRTP_V2.ROLE r ON urm.ROLE_ID = r.ROLE_ID
WHERE u.USER_ID = 44
GROUP BY al.AUDIT_LOG_ID;

