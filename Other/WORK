			+ "NOW() >= START_TIMESTAMP && "
			+ "NOW() <= END_TIMESTAMP && "
			+ "NOW() >= START_TRACKING_DATE && " <-- pengecekan group/client hilangkan
			+ "NOW() <= END_TRACKING_DATE && "   <-- pengecekan group/client hilangkan
			+ "IF(INACTIVE_REASON = '" + InactivateUser.SKIP_CHECKING + "', "
					+ "true, "
					+ "IF(MAX(LAST_ATTEMPT) is null, "
