<?xml version="1.0" encoding="UTF-8"?>
<WL5G3N0:definitions targetNamespace="http://homecredit.net/homerselect/ws/card/management/v2" xmlns:WL5G3N0="http://schemas.xmlsoap.org/wsdl/" xmlns:WL5G3N1="http://homecredit.net/homerselect/ws/card/management/v2" xmlns:WL5G3N2="http://schemas.xmlsoap.org/wsdl/soap/">
  <WL5G3N0:types>
    <xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://homecredit.net/homerselect/ws/common/v2" xmlns:sch0="http://homecredit.net/homerselect/ws/common/v2" xmlns:sch1="http://homecredit.net/homerselect/ws/card/common/v2" xmlns:sch2="http://homecredit.net/homerselect/ws/card/management/v2" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://homecredit.net/homerselect/ws/common/v2" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xs="http://www.w3.org/2001/XMLSchema">
      <xs:element name="ErrorDto">
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="tns:ErrorTypeDto">
              <xs:sequence/>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:complexType name="ErrorTypeDto">
        <xs:annotation>
          <xs:documentation>Error/Exception description</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="errorCode" type="xs:string"/>
          <xs:element minOccurs="0" name="errorMessage" type="xs:string"/>
          <xs:element minOccurs="0" name="errorID" type="xs:string">
            <xs:annotation xml:space="default">
              <xs:documentation>Unique error ID</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="args" type="xs:string"/>
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="CurrencyCodeType">
        <xs:restriction base="xs:string">
          <xs:length value="3"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="MoneyDto">
        <xs:annotation>
          <xs:documentation>Money definition - amount and currency.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="value" type="xs:decimal"/>
          <xs:element name="currency" type="tns:CurrencyCodeType"/>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="PositiveMoneyDto">
        <xs:annotation>
          <xs:documentation>Positive money definition - amount >= 0 and currency.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
          <xs:restriction base="tns:MoneyDto">
            <xs:sequence>
              <xs:element name="value">
                <xs:simpleType>
                  <xs:restriction base="xs:decimal">
                    <xs:minInclusive value="0"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="currency" type="tns:CurrencyCodeType"/>
            </xs:sequence>
          </xs:restriction>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="PropertyDto">
        <xs:sequence>
          <xs:element name="name" type="xs:string"/>
          <xs:element name="value" type="xs:string"/>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="PropertySearchDto">
        <xs:sequence>
          <xs:element name="name" type="xs:string"/>
          <xs:element name="pattern"/>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="StaticDataReferenceDto">
        <xs:sequence>
          <xs:element name="code" type="xs:string"/>
          <xs:element name="version" type="xs:long"/>
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="TerminalTypeDto">
        <xs:annotation>
          <xs:documentation>Card terminal type where the transaction was executed (e.g. transactions without card present).</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:enumeration value="CARD_PRESENT"/>
          <xs:enumeration value="ONLINE">
            <xs:annotation>
              <xs:documentation>card not present, MOTO</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="TransactionTypeDto">
        <xs:restriction base="xs:string">
          <xs:enumeration value="ATM">
            <xs:annotation xml:space="default">
              <xs:documentation>Homer transaction type: ATM Automated Teller Machine withdrawal</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="CSD">
            <xs:annotation xml:space="default">
              <xs:documentation>Homer transaction type: CSD Cash Advance</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="RTL">
            <xs:annotation xml:space="default">
              <xs:documentation>Homer transaction type: RTL Normal Purchase</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="RET">
            <xs:annotation xml:space="default">
              <xs:documentation>Homer transaction type: RET Merchandise Return</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="DEP">
            <xs:annotation xml:space="default">
              <xs:documentation>Homer transaction type: ATM DEP Deposit</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="CAD">
            <xs:annotation xml:space="default">
              <xs:documentation>Deprecated: use DEP instead. Homer transaction type: CAD ATM Cash deposit.
                        - Used only in RU to enable deposit money with credit card</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="INQ">
            <xs:annotation xml:space="default">
              <xs:documentation>Homer transaction type: INQ Balance Inquiry</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="CAV">
            <xs:annotation xml:space="default">
              <xs:documentation>Homer transaction type: CAV Card verification.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="OTH">
            <xs:annotation xml:space="default">
              <xs:documentation>Homer transaction type: OTH Other</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="INS">
            <xs:annotation xml:space="default">
              <xs:documentation>Additional HS Transaction type: INS Insurance calculated by an external insurance module.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="FEE">
            <xs:annotation xml:space="default">
              <xs:documentation>Additional HS Transaction type: FEE calculated by an external system.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="ICD">
            <xs:annotation xml:space="default">
              <xs:documentation>Additional HS Transaction type: Initial Cash Debit Tx.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="IPD">
            <xs:annotation xml:space="default">
              <xs:documentation>Additional HS Transaction type: Initial Pos Debit Tx.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="IIS">
            <xs:annotation xml:space="default">
              <xs:documentation>Additional HS Transaction type: Initial insurance</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="PMT">
            <xs:annotation xml:space="default">
              <xs:documentation>Additional HS Transaction type: Payment.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="GAM">
            <xs:annotation xml:space="default">
              <xs:documentation>Homer transaction type: GAM - Gambling</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="PRE">
            <xs:annotation xml:space="default">
              <xs:documentation>Homer transaction type: PRE Preauthorization</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="MNS">
            <xs:annotation xml:space="default">
              <xs:documentation>Homer transaction type: MNS Money send. Credit side.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="CDCD">
            <xs:annotation xml:space="default">
              <xs:documentation>Cash deposit at cash desk. Transaction without card.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="CWCD">
            <xs:annotation xml:space="default">
              <xs:documentation>Withdrawal cash at cash desk. Transaction without card.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="CDSS">
            <xs:annotation xml:space="default">
              <xs:documentation>Cash deposit through self-service terminal (kiosk). Transaction without card.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="BTIN">
            <xs:annotation xml:space="default">
              <xs:documentation>Acceptation of bank transfer (incoming). Transaction without card.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="BTOUT">
            <xs:annotation xml:space="default">
              <xs:documentation>Execution of internal bank transfer (outgoing). Transaction without card.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="INT_BT_OUT_CL">
            <xs:annotation xml:space="default">
              <xs:documentation>Execution of internal bank transfer (outgoing) for account termination (closing). Transaction without card.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="EXT_BT_OUT_CL">
            <xs:annotation xml:space="default">
              <xs:documentation>Execution of external bank transfer (outgoing) for account termination (closing). Transaction without card.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="PFC_POST">
            <xs:annotation xml:space="default">
              <xs:documentation>Incoming payment from post office. Transaction without card.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="PFC_BANK">
            <xs:annotation xml:space="default">
              <xs:documentation>Incoming payment from foreign bank. Transaction without card.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="PFC_CARD">
            <xs:annotation xml:space="default">
              <xs:documentation>Funds received from foreign card, i.e. NurBank</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="BI">
            <xs:annotation xml:space="default">
              <xs:documentation>Bank income in case of account termination - Debit. Transfer of own funds to bank account.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="SU">
            <xs:annotation xml:space="default">
              <xs:documentation>(Close end account only - Deprecated: use DAD) Bank income in case of account termination needs incoming payment to pay debt(small underpayment) - Credit. Transfer from bank
                        account.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="DAD">
            <xs:annotation xml:space="default">
              <xs:documentation>Bank income in case of account termination needs incoming payment to pay debt(like small underpayment) Deficits and Damages - Credit. Transfer from bank account.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="INS_CLAIM">
            <xs:annotation xml:space="default">
              <xs:documentation>Insurance claim payment.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="TransactionSubTypeDto">
        <xs:annotation xml:space="default">
          <xs:documentation>Transaction sub type, e.g. type of goods purchased by this transaction</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:enumeration value="TW">
            <xs:annotation xml:space="default">
              <xs:documentation>Two wheeler.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="CD">
            <xs:annotation xml:space="default">
              <xs:documentation>Consumer goods.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="CL">
            <xs:annotation xml:space="default">
              <xs:documentation>Cash loan.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="REG_INT">
            <xs:annotation>
              <xs:documentation>Regular interest.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="UN_GP_INT">
            <xs:annotation>
              <xs:documentation>Unused grace period interest.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="UNKNOWN">
            <xs:annotation xml:space="default">
              <xs:documentation>Unknown transaction sub type.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="SourceSystemEnumDto">
        <xs:restriction base="xs:string">
          <xs:enumeration value="BSL"/>
          <xs:enumeration value="RISK"/>
          <xs:enumeration value="COLLECTION"/>
          <xs:enumeration value="CARD"/>
          <xs:enumeration value="ACCOUNT"/>
          <xs:enumeration value="UFO"/>
          <xs:enumeration value="IB">
            <xs:annotation>
              <xs:documentation>Internet Banking</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="TransactionSourceCodeType">
        <xs:annotation>
          <xs:documentation>Unique public code - allocated by the caller.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:maxLength value="128"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="TransactionSourceIdDto">
        <xs:annotation>
          <xs:documentation>Transaction identification in the source system where the card transaction was originated.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="sourceSystem" type="tns:SourceSystemEnumDto">
            <xs:annotation>
              <xs:documentation>Source system identification code.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="sourceTxId" type="tns:TransactionSourceCodeType">
            <xs:annotation>
              <xs:documentation>Transaction id in source system.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="RoundingType">
        <xs:restriction base="xs:string">
          <xs:enumeration value="UP">
            <xs:annotation>
              <xs:documentation>UP - RoundingMode.UP</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="DOWN">
            <xs:annotation>
              <xs:documentation>DOWN - RoundingMode.FLOOR</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="TRUNC">
            <xs:annotation>
              <xs:documentation>TRUNC - RoundingMode.DOWN</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="MATH">
            <xs:annotation>
              <xs:documentation>MATH - RoundingMode.HALF_UP</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
    </xs:schema>
    <xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://homecredit.net/homerselect/ws/card/common/v2" xmlns:crd="http://homecredit.net/homerselect/ws/card/common/v2" xmlns:hs="http://homecredit.net/homerselect/ws/common/v2" xmlns:sch0="http://homecredit.net/homerselect/ws/common/v2" xmlns:sch1="http://homecredit.net/homerselect/ws/card/common/v2" xmlns:sch2="http://homecredit.net/homerselect/ws/card/management/v2" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://homecredit.net/homerselect/ws/card/management/v2" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xs="http://www.w3.org/2001/XMLSchema">
      <xs:import namespace="http://homecredit.net/homerselect/ws/common/v2"/>
      <xs:complexType name="CardDto">
        <xs:annotation>
          <xs:documentation>Card data transfer object.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="pcid" type="crd:PaymentCardId">
            <xs:annotation>
              <xs:documentation>Payment card ID</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="truncatedPan" type="crd:TruncatedPan">
            <xs:annotation>
              <xs:documentation>Truncated (i.e. masked) card number</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="holderCuid" nillable="true" type="crd:Cuid">
            <xs:annotation>
              <xs:documentation>Card holder CUID</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="holderName" nillable="true" type="crd:HolderName">
            <xs:annotation>
              <xs:documentation>Card holder embossed name</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="activeFrom" type="xs:date">
            <xs:annotation>
              <xs:documentation>Date of activation</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="cardType" type="crd:CardTypeDto">
            <xs:annotation>
              <xs:documentation>Card type</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="status" type="crd:CardStatusDto">
            <xs:annotation>
              <xs:documentation>Card status</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="account" nillable="true" type="crd:AccountDto">
            <xs:annotation>
              <xs:documentation>Account</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="validTo" type="xs:date">
            <xs:annotation>
              <xs:documentation>Date with the last validity day</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="CardDetailDto">
        <xs:annotation>
          <xs:documentation>Card detail</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
          <xs:extension base="crd:CardDto">
            <xs:sequence>
              <xs:element minOccurs="0" name="reservationCode" type="xs:long">
                <xs:annotation>
                  <xs:documentation>Card reservation code (e.g. contract or account number)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element maxOccurs="unbounded" minOccurs="0" name="allowedOperations" type="crd:CardOperationDto">
                <xs:annotation>
                  <xs:documentation>Allowed operations on the card</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element maxOccurs="unbounded" minOccurs="0" name="limits" type="crd:LimitDto">
                <xs:annotation>
                  <xs:documentation>Set of limits applied to card</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element maxOccurs="unbounded" name="plastics" type="crd:PlasticDto">
                <xs:annotation>
                  <xs:documentation>Set of all plastics associated with the card (there will be always at least one)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="lastPinChangeRequestDate" type="xs:date"/>
              <xs:element maxOccurs="unbounded" minOccurs="0" name="activeBlocks" type="crd:CardBlockDto"/>
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="PlasticDto">
        <xs:sequence>
          <xs:element name="status" type="crd:PlasticStatusDto"/>
          <xs:element name="validTo" type="xs:date"/>
          <xs:element minOccurs="0" name="renewalDate" type="xs:date"/>
          <xs:element minOccurs="0" name="pickupDate" type="xs:date"/>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="AccountDto">
        <xs:sequence>
          <xs:element name="contractCode" type="crd:ContractCode"/>
          <xs:element name="number" type="crd:AccountNumber"/>
          <xs:element minOccurs="0" name="iban" type="crd:AccountIban"/>
          <xs:element name="type" type="crd:AccountTypeDto"/>
          <xs:element name="currency" type="hs:CurrencyCodeType"/>
          <xs:element name="contractSignatureDate" type="xs:date"/>
          <xs:element name="ownerCuid" type="crd:Cuid"/>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="CardTypeDto">
        <xs:annotation>
          <xs:documentation>Card type</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="id" type="xs:string"/>
          <xs:element name="associationId" type="xs:string"/>
          <xs:element name="kind" type="xs:string"/>
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="CardTypeId">
        <xs:annotation>
          <xs:documentation>Card type (K1,K2,...K9,10,11)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:maxLength value="20"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="LimitDto">
        <xs:annotation>
          <xs:documentation>Card limit data transfer object</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="limitTypeId" type="xs:string">
            <xs:annotation>
              <xs:documentation>Limit type</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="period" type="crd:PeriodDto">
            <xs:annotation>
              <xs:documentation>Limit aggregated per period</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice>
            <xs:element name="currentValueAmount" type="crd:PositiveAmount">
              <xs:annotation>
                <xs:documentation>Current limit value per period - monetary amount</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="currentValueCount" type="xs:integer">
              <xs:annotation>
                <xs:documentation>Current limit value per period - usage count</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="PeriodDto">
        <xs:annotation>
          <xs:documentation>Period of type.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:enumeration value="SINGLE_UPPER"/>
          <xs:enumeration value="SINGLE_LOWER"/>
          <xs:enumeration value="DAY"/>
          <xs:enumeration value="WEEK"/>
          <xs:enumeration value="MONTH"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="CardStatusDto">
        <xs:annotation>
          <xs:documentation>Card status.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:enumeration value="UNCONFIRMED"/>
          <xs:enumeration value="CREATED"/>
          <xs:enumeration value="RESERVED"/>
          <xs:enumeration value="INACTIVE"/>
          <xs:enumeration value="RENEWED"/>
          <xs:enumeration value="ACTIVE"/>
          <xs:enumeration value="BLOCKED"/>
          <xs:enumeration value="DETAIN"/>
          <xs:enumeration value="STOLEN"/>
          <xs:enumeration value="LOST"/>
          <xs:enumeration value="CANCELLED"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="NewCardBlockDto">
        <xs:sequence>
          <xs:element name="cardBlockTypeId" type="xs:string"/>
          <xs:element name="message" nillable="true" type="xs:string"/>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="CardBlockDto">
        <xs:complexContent>
          <xs:extension base="crd:NewCardBlockDto">
            <xs:sequence>
              <xs:element name="blockedFrom" type="xs:dateTime"/>
              <xs:element name="blockedTill" nillable="true" type="xs:dateTime"/>
              <xs:element name="createdBy" type="xs:string"/>
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="PosDeliveryAddressDto">
        <xs:sequence>
          <xs:element name="sellerPlaceCode" type="crd:SellerPlaceCode"/>
          <xs:element minOccurs="0" name="zip" type="crd:Zip"/>
          <xs:element minOccurs="0" name="region" type="crd:Region"/>
          <xs:element minOccurs="0" name="district" type="crd:District"/>
          <xs:element minOccurs="0" name="town" type="crd:Town"/>
          <xs:element minOccurs="0" name="street" type="crd:Street"/>
          <xs:element minOccurs="0" name="houseNumber" type="crd:HouseNumber"/>
          <xs:element minOccurs="0" name="blockNumber" type="crd:BlockNumber"/>
          <xs:element minOccurs="0" name="flatNumber" type="crd:FlatNumber"/>
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="CardOperationDto">
        <xs:annotation>
          <xs:documentation>Card operations</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:enumeration value="RESERVE">
            <xs:annotation>
              <xs:documentation>Reserve card (ReserveCardRequest)</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
